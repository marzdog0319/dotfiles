{
    "editor.fontFamily": "'Ricty Diminished Discord', Menlo, Monaco, 'Courier New', monospace",
    "editor.fontSize": 14,
    "git.autofetch": true,
    "workbench.startupEditor": "newUntitledFile",
    "vim.vimrc.enable": true,
    "vim.useSystemClipboard": true,
    "vim.hlsearch": true,
